<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.text.Font?>
<HBox stylesheets="/style/style.css" fx:controller="ch.supsi.view.info.InfoBarView" alignment="CENTER_LEFT" prefHeight="30.0" prefWidth="200.0" xmlns:fx="http://javafx.com/fxml">
    <children>
        <Label fx:id="moduleOrder" text="Module order">
            <font>
                <Font size="11.0" />
            </font></Label>
        <Region prefHeight="30.0" prefWidth="30.0" />
        <Label fx:id="v3" text="v3 RAW">
            <font>
                <Font size="11.0" />
            </font></Label>
        <Region prefHeight="30.0" prefWidth="42.0" />
        <HBox prefHeight="100.0" prefWidth="67.0">
            <children>
                <Button fx:id="bottomButton1" onAction="#bb1Action" mnemonicParsing="false" styleClass="bottomButton">
                    <HBox.margin>
                        <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
                    </HBox.margin>
                    <graphic>
                        <ImageView fitHeight="25" fitWidth="25" preserveRatio="true">
                            <image>
                                <Image url="@/images/buttons/bottom/bb1.png" />
                            </image>
                        </ImageView>
                    </graphic>
                </Button>
                <Button fx:id="bottomButton2" onAction="#bb2Action" mnemonicParsing="false" styleClass="bottomButton">
                    <HBox.margin>
                        <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
                    </HBox.margin>
                    <graphic>
                        <ImageView fitHeight="25" fitWidth="25" preserveRatio="true">
                            <image>
                                <Image url="@/images/buttons/bottom/bb2.png" />
                            </image>
                        </ImageView>
                    </graphic>
                </Button>
            </children>
        </HBox>
    </children>
    <VBox.margin>
        <Insets left="5.0" right="5.0" />
    </VBox.margin>
</HBox>
